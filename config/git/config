[pull]
    rebase = true
[alias]
    a = add
    ac = !git add . && git commit -m 
    cm = commit -m
    co = checkout
    cp = cherry-pick
    d = diff
    poh = push -u origin HEAD
    p = push
    sc = switch -c
    sw = switch
    s = status
    diff-p = diff -- . ':(exclude)package-lock*'
[init]
    defaultBranch = main

